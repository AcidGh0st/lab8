<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="bp" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BTreeTourController">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="750.0">
         <children>
            <Pane layoutY="64.0" prefHeight="77.0" prefWidth="750.0">
               <children>
                  <Label layoutX="14.0" layoutY="48.0" />
                  <Pane fx:id="treePane" layoutX="2.0" layoutY="-62.0" prefHeight="590.0" prefWidth="750.0" />
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </center>
   <top>
      <Pane prefHeight="111.0" prefWidth="750.0" BorderPane.alignment="CENTER">
         <children>
            <Pane prefHeight="70.0" prefWidth="750.0" style="-fx-background-color: F4E2FE};">
               <children>
                  <Label layoutX="14.0" layoutY="14.0" prefHeight="41.0" prefWidth="731.0" style="-fx-background-color: F4E2FE;" text="Graphic Binary Tree- Transversal Tour">
                     <font>
                        <Font name="Forte" size="46.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane layoutY="70.0" prefHeight="45.0" prefWidth="750.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="47.0" prefWidth="750.0">
                     <children>
                        <Button fx:id="btn_Randomize" mnemonicParsing="false" onAction="#RandomizeOnAction" text="Randomize">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="btn_PreOrder" mnemonicParsing="false" onAction="#PreOrderOnAction" text="Pre Order">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="btn_InOrder" mnemonicParsing="false" onAction="#InOrderOnAction" text="In Order">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="btn_PostOrder" mnemonicParsing="false" onAction="#PostOrderOnAction" text="Post Order">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </Pane>
         </children>
      </Pane>
   </top>
</BorderPane>
